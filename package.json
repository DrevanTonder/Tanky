{
  "name": "tanky",
  "license": "MIT",
  "scripts": {
    "server": "node src/server/index.js",
    "watch:client": "npx webpack --watch",
    "watch:server": "nodemon"
  },
  "dependencies": {
    "express": "4.16.3",
    "phaser": "3.10.1",
    "socket.io": "2.1.1",
    "uuid": "3.2.1"
  },
  "devDependencies": {
    "@types/express": "4.16.0",
    "@types/node": "10.3.3",
    "@types/socket.io": "1.4.35",
    "@types/socket.io-client": "1.4.32",
    "copy-webpack-plugin": "4.5.1",
    "eslint": "4.19.1",
    "gts": "^0.6.0",
    "typescript": "~2.8.0",
    "webpack": "4.12.0",
    "webpack-cli": "3.0.8"
  },
  "nodemonConfig": {
    "watch": [
      "src/server",
      "src/imports"
    ],
    "exec": "yarn server",
    "ext": "*.js"
  }
}
